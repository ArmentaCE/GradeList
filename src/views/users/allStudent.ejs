<% if (typeof alumnos != 'undefined' && alumnos.length != 0) { %>
    <% for( let i = 0; i < messages.length; i++ ) { %>
        <% if (alumnos[i].length != 0) { %>
            <div class="error">
                <p><%= alumnos[i] %></p>
                <button class="botonBorrar" onclick="quitar(this)"> <b>&times;</b> </button>
            </div>
        <% } %>
    
    <% } %>
<% } %>